http://apps.localhost, apps.solsort.com {
  root /apps
}

http://piwik.localhost, piwik.solsort.com, piwik.rasmuserik.com {
  proxy / http://piwik {
    proxy_header Host {host}
    proxy_header X-Real-IP {remote}
    proxy_header X-Forwarded-Proto {scheme}
  }
  cors
}
